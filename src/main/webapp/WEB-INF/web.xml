<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Online Booking System</display-name>
	
	<servlet>
		<servlet-name>Route</servlet-name>
		<jsp-file>/WEB-INF/view/Route.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>Schedule</servlet-name>
		<jsp-file>/WEB-INF/view/Schedule.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>Result</servlet-name>
		<jsp-file>/WEB-INF/view/Result.jsp</jsp-file>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Route</servlet-name>
		<url-pattern>/Route</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Schedule</servlet-name>
		<url-pattern>/Schedule</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Result</servlet-name>
		<url-pattern>/Result</url-pattern>
	</servlet-mapping>
</web-app>
